#: ../../extmisc.rst:648
msgid "count=BLOCKS"
msgstr ""

#: ../../extmisc.rst:650
msgid "Copy only this many blocks of data, rather than the entire INFILE."
msgstr ""

#: ../../extmisc.rst:653
msgid "conv=CONVERSION"
msgstr ""

#: ../../extmisc.rst:655
msgid ""
"Type of conversion to be applied to INFILE data before copying operation."
