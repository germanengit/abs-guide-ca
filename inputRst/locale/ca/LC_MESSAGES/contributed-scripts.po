#: ../../contributed-scripts.rst:653
msgid ""
"# Assign letter values. # Remove duplicate numbers, except when separated by"
" vowels. # Ignore vowels, except as separators, so delete them last. # "
"Ignore 'w' and 'h', even as separators, so delete them first. # # The above "
"command substitution lays more pipe than a plumber <g>."
msgstr ""

#: ../../contributed-scripts.rst:660
msgid "}"
msgstr ""

#: ../../contributed-scripts.rst:663
msgid "input_name=\"$1\" echo echo \"Name = $input_name\""
msgstr ""

#: ../../contributed-scripts.rst:668
msgid ""
"# Change all characters of name input to lowercase. # "
"------------------------------------------------ name=$( echo $input_nametr "
"A-Z a-z ) # ------------------------------------------------ # Just in case "
"argument to script is mixed case."
msgstr ""

#: ../../contributed-scripts.rst:675
msgid ""
"# Prefix of soundex code: first letter of name. # "
"--------------------------------------------"
msgstr ""

#: ../../contributed-scripts.rst:679
msgid ""
"char_pos=0                     # Initialize character position. "
"prefix0=${name:$char_pos:1} prefix=`echo $prefix0tr a-z A-Z`"
msgstr ""

#: ../../contributed-scripts.rst:682
msgid "# Uppercase 1st letter of soundex."
msgstr ""

#: ../../contributed-scripts.rst:684
msgid ""
"let \"char_pos += 1\"            # Bump character position to 2nd letter of "
"name. name1=${name:$char_pos}"
msgstr ""

#: ../../contributed-scripts.rst:688
msgid ""
"# ++++++++++++++++++++++++++ Exception Patch ++++++++++++++++++++++++++++++ "
"#  Now, we run both the input name and the name shifted one char #+ to the "
"right through the value-assigning function. #  If we get the same value out,"
" that means that the first two characters #+ of the name have the same value"
" assigned, and that one should cancel. #  However, we also need to test "
"whether the first letter of the name #+ is a vowel or 'w' or 'h', because "
"otherwise this would bollix things up."
msgstr ""

#: ../../contributed-scripts.rst:696
msgid "char1=`echo $prefixtr A-Z a-z`    # First letter of name, lowercased."
msgstr ""

#: ../../contributed-scripts.rst:698
msgid ""
"assign_value $name s1=$value assign_value $name1 s2=$value assign_value "
"$char1 s3=$value s3=9$s3                              #  If first letter of "
"name is a vowel"
msgstr ""

#: ../../contributed-scripts.rst:705
msgid "#+ or 'w' or 'h', #+ then its \"value\" will be null (unset)."
msgstr ""

#: ../../contributed-scripts.rst:707
msgid ""
"#+ Therefore, set it to 9, an otherwise #+ unused value, which can be tested"
" for."
msgstr ""

#: ../../contributed-scripts.rst:711
msgid "if [[ \"$s1\" -ne \"$s2\" |\"$s3\" -eq 9 ]] then"
msgstr ""

#: ../../contributed-scripts.rst:713
msgid "suffix=$s2"
msgstr ""

#: ../../contributed-scripts.rst:715
msgid "suffix=${s2:$char_pos}"
msgstr ""

#: ../../contributed-scripts.rst:716
msgid ""
"fi # ++++++++++++++++++++++ end Exception Patch "
"++++++++++++++++++++++++++++++"
msgstr ""

#: ../../contributed-scripts.rst:720
msgid "padding=000                    # Use at most 3 zeroes to pad."
msgstr ""

#: ../../contributed-scripts.rst:723
msgid "soun=$prefix$suffix$padding    # Pad with zeroes."
msgstr ""

#: ../../contributed-scripts.rst:725
msgid ""
"MAXLEN=4                       # Truncate to maximum of 4 chars. "
"soundex=${soun:0:$MAXLEN}"
msgstr ""

#: ../../contributed-scripts.rst:728
msgid "echo \"Soundex = $soundex\""
msgstr ""

#: ../../contributed-scripts.rst:730
msgid "echo"
msgstr ""

#: ../../contributed-scripts.rst:732
msgid ""
"#  The soundex code is a method of indexing and classifying names #+ by "
"grouping together the ones that sound alike. #  The soundex code for a given"
" name is the first letter of the name, #+ followed by a calculated three-"
"number code. #  Similar sounding names should have almost the same soundex "
"codes."
msgstr ""

#: ../../contributed-scripts.rst:738
msgid ""
"#   Examples: #   Smith and Smythe both have a \"S-530\" soundex. #   "
"Harrison = H-625 #   Hargison = H-622 #   Harriman = H-655"
msgstr ""

#: ../../contributed-scripts.rst:744
msgid ""
"#  This works out fairly well in practice, but there are numerous anomalies."
" # # #  The U.S. Census and certain other governmental agencies use soundex,"
" #  as do genealogical researchers. # #  For more information, #+ see the "
"\"National Archives and Records Administration home page\", #+ "
"http://www.nara.gov/genealogy/soundex/soundex.html"
msgstr ""

