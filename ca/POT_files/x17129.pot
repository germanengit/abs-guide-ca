msgid "awk '{ $0=$1 \"\\n\" $2"
msgstr ""

#: ../../x17129.rst:508
# 6b558f649a5049ca9ad360e00195b4d8
msgid "; if (/line.1/) {print}} ' << EOF"
msgstr ""

#: ../../x17129.rst:510
# 135a0f3853ba4cf581348551da2ab789
msgid "line 1 line 2 EOF # OUTPUT: # line # 1"
msgstr ""

