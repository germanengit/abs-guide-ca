msgstr ""

#: ../../standard-options.rst:84
# 63909faf2a1d4132aed278d3d3690651
msgid "In **cp** , **mv** , **rm** :"
msgstr ""

#: ../../standard-options.rst:88
# 28885e4ed97f4babbb559d60e11d4210
msgid "``        --force       ``"
msgstr ""

#: ../../standard-options.rst:90
# aebb4279f34f4ee6a5b5be3bdd6160a5
msgid "*Force* : force overwrite of target file(s)."
msgstr ""

#: ../../standard-options.rst:94
# 0069d80f348e4b27aadba843fe38678e
msgid "|Caution"
msgstr ""

#: ../../standard-options.rst:96
# fbdec8dca6ef47f7bec8691c72de962c
msgid "Many UNIX and Linux utilities deviate from this \"standard,\" so it is dangerous to *assume* that a given option will behave in a standard way. Always check the man page for the command in question when in doubt."
msgstr ""

#: ../../standard-options.rst:103
# 36d6705bac754476944bfb709508de7e
msgid "A complete table of recommended options for the GNU utilities is available at `the GNU standards page <http://www.gnu.org/prep/standards/>`__ ."
