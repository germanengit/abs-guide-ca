msgstr ""

#: ../../x23170.rst:168
# adadadd1a4e44435b6b930ca749d4f95
msgid "Delete from beginning of input up to, and including first blank line."
msgstr ""

#: ../../x23170.rst:170
# 0ab032bece5944a180f2c89e8a85da98
msgid "``        /Jones/p       ``"
msgstr ""

#: ../../x23170.rst:172
# dff7340be78c4e938520e65d7f366106
msgid "Print only lines containing \"Jones\" (with -n option)."
msgstr ""

#: ../../x23170.rst:174
# ac7c8f8cd9da4588b82afc18e29d55f3
msgid "``        s/Windows/Linux/       ``"
msgstr ""

#: ../../x23170.rst:176
# 70c9331fe3d845e39451a2528af2542d
msgid "Substitute \"Linux\" for first instance of \"Windows\" found in each input line."
msgstr ""

#: ../../x23170.rst:179
# cd0ce93909f540408d27a34a6dff21fe
msgid "``        s/BSOD/stability/g       ``"
msgstr ""

#: ../../x23170.rst:181
# 5488a35f915c41aa880a1e46dfd00113
msgid "Substitute \"stability\" for every instance of \"BSOD\" found in each input line."
msgstr ""

#: ../../x23170.rst:184
# 68ecd001e75d4c57b324a37ef9dadb28
msgid "``        s/ *$//       ``"
msgstr ""

#: ../../x23170.rst:186
# c98f4c93dcb747e1ac920bc2faa62a4b
msgid "Delete all spaces at the end of every line."
msgstr ""

#: ../../x23170.rst:188
# 2ea1691a103a439c8cd2e67f54318773
msgid "``        s/00*/0/g       ``"
msgstr ""

#: ../../x23170.rst:190
# 10638839adae4e95b5bc614dac74e0dd
msgid "Compress all consecutive sequences of zeroes into a single zero."
msgstr ""

#: ../../x23170.rst:192
# 3b89f4c73732486e80c2a917845aa70c
msgid "``        echo \"Working on it.\"sed -e '1i How far are you along?'       ``"
msgstr ""

#: ../../x23170.rst:194
# c3845bc21ace4da2bb8af5ee00e980c6
msgid "Prints \"How far are you along?\" as first line, \"Working on it\" as second."
msgstr ""

#: ../../x23170.rst:197
# d3effa441ab54ae199a6aaf21d7e6e38
msgid "``        5i 'Linux is great.' file.txt       ``"
msgstr ""

#: ../../x23170.rst:199
# 51400455d0794795a75aaece6183310a
msgid "Inserts 'Linux is great.' at line 5 of the file file.txt."
msgstr ""

#: ../../x23170.rst:201
# a5cee8fa8e5b4c4183780bda73c0bffc
msgid "``        /GUI/d       ``"
msgstr ""

#: ../../x23170.rst:203
# 27525371e189403a927cc2eec9573158
msgid "Delete all lines containing \"GUI\" ."
msgstr ""

#: ../../x23170.rst:205
# e22af56c702c4bd58afd121ac472f2cc
msgid "``        s/GUI//g       ``"
msgstr ""

#: ../../x23170.rst:207
# 92c4616e8ef44d8fa7bcd5a3ca997dfe
msgid "Delete all instances of \"GUI\" , leaving the remainder of each line intact."
msgstr ""

#: ../../x23170.rst:212
# dd99b3157af944079cc0cebdea38662c
msgid "Substituting a zero-length string for another is equivalent to deleting that string within a line of input. This leaves the remainder of the line intact. Applying ``             s/GUI//           `` to the line"
msgstr ""

