msgstr ""

#: ../../terminalccmds.rst:32
# 6acae7614088442a925d3318b2a44aa8
msgid "``           smul          `` , to underline text in the terminal"
msgstr ""

#: ../../terminalccmds.rst:34
# 999a5f93643b45ccb1c0d86d1f61070a
msgid "``           smso          `` , to render text in reverse"
msgstr ""

#: ../../terminalccmds.rst:36
# e9a55b679a734fb68ad91e912742933b
msgid "``           sgr0          `` , to reset the terminal parameters (to normal), without clearing the screen"
msgstr ""

