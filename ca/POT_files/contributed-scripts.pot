msgstr ""

#: ../../contributed-scripts.rst:688
# 37f214a082544723a3884b869cce0ae4
msgid "# ++++++++++++++++++++++++++ Exception Patch ++++++++++++++++++++++++++++++ #  Now, we run both the input name and the name shifted one char #+ to the right through the value-assigning function. #  If we get the same value out, that means that the first two characters #+ of the name have the same value assigned, and that one should cancel. #  However, we also need to test whether the first letter of the name #+ is a vowel or 'w' or 'h', because otherwise this would bollix things up."
msgstr ""

#: ../../contributed-scripts.rst:696
# 4b4a5a055eb248d89b252b1f50d20944
msgid "char1=`echo $prefixtr A-Z a-z`    # First letter of name, lowercased."
msgstr ""

#: ../../contributed-scripts.rst:698
# 42f1503219184ff5a14a857c423efa11
msgid "assign_value $name s1=$value assign_value $name1 s2=$value assign_value $char1 s3=$value s3=9$s3                              #  If first letter of name is a vowel"
msgstr ""

#: ../../contributed-scripts.rst:705
# ac11425ba06f40b48f68056596251507
msgid "#+ or 'w' or 'h', #+ then its \"value\" will be null (unset)."
msgstr ""

#: ../../contributed-scripts.rst:707
# dccc2ec9255e44bab5c2984fe3a7c2e3
msgid "#+ Therefore, set it to 9, an otherwise #+ unused value, which can be tested for."
msgstr ""

#: ../../contributed-scripts.rst:711
# 10a2a59a0fa3436fbd82de8d28555259
msgid "if [[ \"$s1\" -ne \"$s2\" |\"$s3\" -eq 9 ]] then"
msgstr ""

#: ../../contributed-scripts.rst:713
# f68431aaf70443759848eaed11dfcf98
msgid "suffix=$s2"
msgstr ""

#: ../../contributed-scripts.rst:715
# 1aafc52a0a5d43f8b5816b2244831604
msgid "suffix=${s2:$char_pos}"
msgstr ""

#: ../../contributed-scripts.rst:716
# 9b2319a9c9d7457eac5f5f5520264007
msgid "fi # ++++++++++++++++++++++ end Exception Patch ++++++++++++++++++++++++++++++"
msgstr ""

#: ../../contributed-scripts.rst:720
# 3d6a95a5b20f4bdeb25640696fea9d7e
msgid "padding=000                    # Use at most 3 zeroes to pad."
msgstr ""

#: ../../contributed-scripts.rst:723
